	Doi = {10.1109/MITE.2016.024},
	Keywords = {computer aided instruction;computer literacy;computer networks;computer science education;digital simulation;educational courses;protocols;teaching;telecommunication engineering education;packet tracer simulation tool;PT simulation tool;computer network concepts;education system;information and communication technology;computer network course;protocols formats;computer network concept teaching;active learning;NAT;Subnetting;V-LAN setting;TCP/UDP services;packet format details;Computer networks;Protocols;Tools;Network topology;Computational modeling;Topology;Education;packet tracer;simulation;network;protocol},
	Month = {Dec},
	Pages = {71-76},
	Title = {Packet Tracer Simulation Tool as Pedagogy to Enhance Learning of Computer Network Concepts},
	Year = {2016},
	Bdsk-File-1 = {},
	Bdsk-File-2 = {},
	Bdsk-Url-1 = {https://doi.org/10.1109/MITE.2016.024}}

