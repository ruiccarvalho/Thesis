	Journal = {IEEE/ACM Transactions on Networking},
	Keywords = {Internet;protocols;digital simulation;telecommunication traffic;telecommunication congestion control;reviews;research initiatives;Internet simulation;network traffic;protocols;network congestion level;simulation parameter space;collaborative research;network simulator;heterogeneous network;Internet;IP networks;Protocols;Analytical models;Traffic control;Collaboration;Computational modeling;Particle measurements;Intelligent networks;Telecommunication traffic},
	Month = {Aug},
	Number = {4},
